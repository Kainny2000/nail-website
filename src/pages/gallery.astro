---
import PicturesGrid from "../components/PicturesGrid.astro";
import Layout from '../layouts/Layout.astro';
import { getImage } from 'astro:assets';
import backgroundImageSrc from '../assets/Image (45).jpeg';
const backgroundImage = await getImage({ src: backgroundImageSrc });
---

<Layout className="layout-gallery" backgroundImage={backgroundImage.src}>
    <PicturesGrid />
</Layout>

