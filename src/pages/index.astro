---
import Layout from '../layouts/Layout.astro';
import PictureCard from '../components/PictureCard.astro';
const picturesImported = import.meta.glob('../assets/*.jpeg', { eager: true }) as Record<string, { default: any }>;
const images = Object.values(picturesImported);

---

<Layout>
	{images.map((image) => 
		<PictureCard src={image.default} alt="A beautiful picture"></PictureCard>
	)}
</Layout>
